<Window x:Class="MangosTEx.Views.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		xmlns:viewModels="clr-namespace:MangosTEx.ViewModels"
        Title="Sample App"
        WindowStartupLocation="CenterScreen"
        Width="800"
        Height="600"
        >
    <Window.Resources>
        <viewModels:MainWindowViewModel x:Key="ViewModel" />
    </Window.Resources>

    <Grid DataContext="{StaticResource ViewModel}">
        <!--<Control Style="{StaticResource ItemsGridStyle}" />-->
        <Control Style="{StaticResource GameObjectsGridStyle}" />
    </Grid>
</Window>
